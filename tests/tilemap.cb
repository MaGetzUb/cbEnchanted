
Const SW = 640
Const SH = 480
SCREEN SW, SH
Const _FPS = 70

Repeat 
    ClearText 
    Text 0, 0, FPS()
    a = a + 1
    
    //Line SW/2, SH/2, SW/2 + Cos(a) * 200, SH/2 - Sin(a) * 200
    
    If Timer() > hetki + (1000.0/_FPS) Then 
        Wait RoundDown(1000.0/_FPS)
        hetki = Timer()
    EndIf 
    
    
DrawScreen
Forever 