// cbEnchanted custom function group
Const CBE_ENET_FUNCTION_GROUP = 3437

// Used to determine invalid socket/packet/host handle
Const CBE_ENET_NULL           = 0

// Defined in enet.h
Const ENET_HOST_ANY           = 0
Const ENET_PORT_ANY           = 0

// ENetPacket flags as defined in enet.h
Const ENET_PACKET_FLAG_RELIABLE               = 1
Const ENET_PACKET_FLAG_UNSEQUENCED            = 2
Const ENET_PACKET_FLAG_NO_ALLOCATE            = 4
Const ENET_PACKET_FLAG_UNRELIABLE_FRAGMENT    = 8
Const ENET_PACKET_FLAG_SENT                   = 256

// ENetEvent types as defined in enet.h
Const ENET_EVENT_TYPE_NONE                    = 0
Const ENET_EVENT_TYPE_CONNECT                 = 1
Const ENET_EVENT_TYPE_DISCONNECT              = 2
Const ENET_EVENT_TYPE_RECEIVE                 = 3

// Imitates ENetAddress struct defined in enet.h
Type ENetAddress
    Field _port         As Short
    Field _host         As Integer
End Type

// Imitates ENetBuffer struct defined in win32.h/unix.h
Type ENetBuffer
    Field _dataLength   As Integer
    Field _data         As Integer // MEMBlock handle for MEMBlock that is clone of ENetBuffer->data
End Type

// Imitates ENetEvent struct defined in enet.h
Type ENetEvent
    Field _type         As Byte
    Field _peer         As Integer // Peer handle used to access ENetPeer
    Field _channelID    As Byte
    Field _data         As Integer
    Field _packet       As Integer // Packet handle used to access ENetPacket
End Type

